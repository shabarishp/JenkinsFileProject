pipeline {
    agent any

        stages {
            stage ('Compile Stage') {
                        steps {
                               sh 'mvn clean compile'
                           }
                       }
            stage ('Testing stage') {
                        steps {
                               sh 'mvn test'
                              }
                           }
            stage ('package Stage') {
                        steps {
                               sh 'mvn package'
                              }
                           }
               }
          }
